<Window x:Class="PenteGame.GameEnd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PenteGame"
        mc:Ignorable="d"
        Title="GameEnd" Height="450" Width="800">
	<Viewbox>
		<StackPanel>
			<Label Name="lblWinInfo" Height="30"></Label>
			<Button Content="New Game" Name="btnNewGame" Click="btnNewGame_Click" Height="25"></Button>
			<Button Content="Main Menu" Name="btnMainMenu" Click="btnMainMenu_Click" Height="25"></Button>
		</StackPanel>
	</Viewbox>
</Window>
