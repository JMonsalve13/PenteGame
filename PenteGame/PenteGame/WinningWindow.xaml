<Window x:Class="PenteGame.WinningWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PenteGame"
        mc:Ignorable="d"
        Title="WinningWindow" Height="450" Width="800">
    <Viewbox>
        <StackPanel>
            <Label Name="playerWonLbl" HorizontalAlignment="Center" Margin="10" FontSize="40" FontFamily="Times New Roman"></Label>
            <Button Name="btnStartGame" Content="Main Menu" Height="30" Width="70" Margin="10"></Button>
            <Button Name="btnHelpScreen" Content="Restart" Height="30" Width="50" Margin="10"></Button>
            <Button Name="btnQuitGame" Content="Quit" Height="30" Width="30" Margin="10"></Button>
        </StackPanel>
    </Viewbox>
</Window>
